    "@types/fs-extra": "^11.0.4",
    "@types/marked": "^5.0.2",
    "@types/node": "^22.15.30",
    "playwright": "^1.40.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  },
  "engines": {
    "node": ">=22.0.0"
