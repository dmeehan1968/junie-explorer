  "description": "Utility App for drilling into log files for JetBrains Junie",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "NODE_OPTIONS=\"--loader ts-node/esm --no-warnings --enable-source-maps\" node src/index.ts",
